<div class="jumbotron jumbotron-fluid" id="header-jumbo">
  <div class="container text-center" id="header-img">
    <h1 class="display-4 name">VICTORIA LEAGUE</h1>
    <h4 class="sub-header">Full-stack web developer with a passion for making things both beautiful and highly functional.</h4>
  </div>
</div>

<div class="container" id="index-container">

    {{!-- PROJECT SECTION --}}
    <div class="row justify-content-center" id="projects">
        <div class="col-sm-10 text-center">
            <h2>Projects</h2>
    
            <div class="row justify-content-center mt-5">
                <div class="col-sm-10">
                    <div class="row justify-content-center">
                        {{#each projects}}
                            {{> projectsprev/projectsprev}}
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>

    {{!-- SKILLS SECTION --}}
    <div class="row justify-content-center pb-5" id="skills">
        <div class="col-sm-7 text-center">
            <h2 class="mb-5">Technologies</h2>
        
            <div class="row">
                <div class="col-md-3 text-center mb-3">
                    <i class="fab fa-js fa-7x tech-icon" data-toggle="tooltip" data-placement="top" title="Javascript ES6">
                    </i> 
                </div>

                <div class="col-md-3 text-center mb-3">
                    <i class="fab fa-react fa-7x tech-icon" data-toggle="tooltip" data-placement="top" title="React.js"></i>
                </div>

                <div class="col-md-3 text-center mb-3">
                    <i class="fab fa-node fa-6x tech-icon" data-toggle="tooltip" data-placement="top" title="Node.js"></i>
                </div>

                <div class="col-md-3 text-center mb-3">
                    <img src="/img/mysql icon.png" data-toggle="tooltip" data-placement="top" title="MySQL">
                </div>
            </div>

            <div class="row">
                <div class="col-md-3 text-center mb-3">
                    <i class="fab fa-html5 fa-7x tech-icon" data-toggle="tooltip" data-placement="bottom" title="HTML 5"></i>
                </div>

                <div class="col-md-3 text-center mb-3">
                    <i class="fab fa-css3-alt fa-7x tech-icon" data-toggle="tooltip" data-placement="bottom" title="CSS 3"></i>
                </div>

                <div class="col-md-3 text-center mb-3">
                    <img src="/img/photoshop icon.png" data-toggle="tooltip" data-placement="bottom" title="Photoshop">
                </div>

                <div class="col-md-3 text-center mb-3">
                    <img src="/img/indesign icon.png" data-toggle="tooltip" data-placement="bottom" title="InDesign">
                </div>
            </div>

            <div class="row mt-5">
                <div class="col-sm-12 text-center">
                    <h2> Skills </h2>
                </div>                
            </div>

            <div class="row justify-content-center mt-5">
                <div class="col-sm-4">
                    <p class="skill-txt">Git</p>
                    <p class="skill-txt">Marketing</p>
                    <p class="skill-txt">Writing</p>
                </div>

                <div class="col-sm-4">
                    <p class="skill-txt">Project Management</p>
                    <p class="skill-txt">Digital Design</p>
                    <p class="skill-txt">Event Planning</p>
                </div>
            </div>
        </div>
    </div>

    <div class="row justify-content-center pb-5" id="work">
        <div class="col-sm-12 text-center pr-0">
            <h2>Experience and Education</h2>

            <div class="row mt-5" id="timeline-row">
                {{#each timelineItems}}
                    {{> timeline/timelineItem}}
                {{/each}}
            </div>
        </div>        
    </div>

    {{!-- CONTACT SECTION --}}
    <div class="row justify-content-center pb-5 mb-0" id="contact">
        <div class="col-sm-7 text-center">
            <h2 class="mb-5">Contact</h2>

            <div class="row">
                <div class="col-sm-6 text-right pr-5 contact-col">
                    <h5 class="contact-head">Email: </h5>
                </div>
                <div class="col-sm-6 text-left pl-4 contact-col">
                    <h5 class="contact-info"><a class="contact-link" href="mailto:vleague2@gmail.com">vleague2@gmail.com</a></h5>
                    <br>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-6 text-right pr-5 contact-col">
                    <h5 class="contact-head">LinkedIn: </h5>
                </div>
                <div class="col-sm-6 text-left pl-4 contact-col">
                    <h5 class="contact-info"><a class="contact-link" href="https://www.linkedin.com/in/victorialeague/">https://www.linkedin.com/in/victorialeague/</a></h5>
                    <br>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-6 text-right pr-5 contact-col">
                    <h5 class="contact-head">Github: </h5>
                </div>
                <div class="col-sm-6 text-left pl-4 contact-col">
                    <h5 class="contact-info"><a class="contact-link" href="https://github.com/vleague2">https://github.com/vleague2</a></h5>
                    <br>
                </div>
            </div>
        </div>
    </div>

</div>